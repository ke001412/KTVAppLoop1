<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
   
     >
     <LinearLayout 
         android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    android:layout_marginTop="15dp"
    android:layout_marginBottom="15dp"
     android:layout_marginLeft="30dp"
    android:layout_marginRight="30dp"
         >
     <LinearLayout 
         android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:layout_weight="2"
         >
         <RelativeLayout 
             android:id="@+id/home_center_gexing"
                 android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gexing"
    android:layout_weight="3"
    android:layout_margin="1dp"
             >
             <TextView 
                 android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="@string/home_center_gexing"
    android:layout_alignParentBottom="true"
    android:layout_alignParentRight="true"
    style="@style/NormalWhiteText18"
     android:layout_margin="1dp"
                 />
         </RelativeLayout>
          <RelativeLayout 
             android:id="@+id/home_center_xinge"
                 android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/xinge"
    android:layout_weight="5"
    android:layout_margin="1dp"
             >
             <TextView 
                 android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="@string/home_center_xinge"
    android:layout_alignParentBottom="true"
    android:layout_alignParentRight="true"
     android:layout_margin="1dp"
    style="@style/NormalWhiteText18"
                 />
         </RelativeLayout>
     </LinearLayout>
  
     
     <LinearLayout 
          android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:layout_weight="2"
         >
     <LinearLayout 
          android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    android:layout_weight="3"
         >
         <RelativeLayout 
              android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/quzhong"
    android:layout_weight="3"
    android:layout_margin="1dp"
    android:id="@+id/home_center_leibie"
             >
               <TextView 
                 android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="@string/home_center_leibie"
    android:layout_alignParentBottom="true"
    android:layout_alignParentRight="true"
     android:layout_margin="1dp"
    style="@style/NormalWhiteText18"
                 />
         </RelativeLayout>
          <RelativeLayout 
              android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/paihang"
    android:layout_weight="3"
    android:layout_margin="1dp"
    android:id="@+id/home_center_paihang"
             >
               <TextView 
                 android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="@string/home_center_paihang"
    android:layout_alignParentBottom="true"
    android:layout_alignParentRight="true"
     android:layout_margin="1dp"
    style="@style/NormalWhiteText18"
                 />
         </RelativeLayout>
     </LinearLayout>
     <RelativeLayout 
              android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/geming"
    android:layout_weight="1.5"
    android:layout_margin="1dp"
    android:id="@+id/home_center_geming"
             >
               <TextView 
                 android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="@string/home_center_geming"
    android:layout_alignParentBottom="true"
    android:layout_alignParentRight="true"
     android:layout_margin="1dp"
    style="@style/NormalBlackText18"
                 />
         </RelativeLayout>
     </LinearLayout>
     
     
     
     <LinearLayout 
         android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
     android:layout_weight="2"
         >
      <LinearLayout 
          android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    android:layout_weight="2"
         >
         <RelativeLayout 
              android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/hd"
    android:layout_weight="3"
    android:layout_margin="1dp"
    android:id="@+id/home_center_gaoqing"
             >
               <TextView 
                 android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="@string/home_center_gaoqing"
    android:layout_alignParentBottom="true"
    android:layout_alignParentRight="true"
    style="@style/NormalWhiteText18"
     android:layout_margin="1dp"
                 />
         </RelativeLayout>
          <RelativeLayout 
              android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/shezhi"
    android:layout_weight="3"
    android:layout_margin="1dp"
    android:id="@+id/home_center_shezhi"
             >
               <TextView 
                 android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="@string/home_center_shezhi"
    android:layout_alignParentBottom="true"
    android:layout_alignParentRight="true"
    style="@style/NormalWhiteText18"
    android:layout_margin="1dp"
                 />
         </RelativeLayout>
     </LinearLayout>
     
        <RelativeLayout 
              android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/remen"
    android:layout_weight="2"
    android:layout_margin="1dp"
    android:id="@+id/home_center_remen"
             >
               <TextView 
                 android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="@string/home_center_remen"
    android:layout_alignParentBottom="true"
    android:layout_alignParentRight="true"
    style="@style/NormalWhiteText18"
     android:layout_margin="1dp"
                 />
         </RelativeLayout>
      
         <LinearLayout 
          android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    android:layout_weight="2"
         >
         <RelativeLayout 
              android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/wuqu"
    android:layout_weight="3"
    android:layout_margin="1dp"
    android:id="@+id/home_center_wuqu"
             >
               <TextView 
                 android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="@string/home_center_wuqu"
    android:layout_alignParentBottom="true"
    android:layout_alignParentRight="true"
    style="@style/NormalWhiteText18"
     android:layout_margin="1dp"
                 />
         </RelativeLayout>
          <RelativeLayout 
              android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/shoucang"
    android:layout_weight="3"
    android:layout_margin="1dp"
    android:id="@+id/home_center_shoucang"
             >
               <TextView 
                 android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="@string/home_center_shoucang"
    android:layout_alignParentBottom="true"
    android:layout_alignParentRight="true"
    style="@style/NormalWhiteText18"
     android:layout_margin="1dp"
                 />
         </RelativeLayout>
     </LinearLayout>
     </LinearLayout>
     </LinearLayout>
</RelativeLayout>
