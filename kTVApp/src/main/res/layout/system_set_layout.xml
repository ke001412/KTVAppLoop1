<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/houtai"
    android:id="@+id/set_layout"
    >
    <TextView 
          android:layout_width="match_parent"
    android:layout_height="60dp"
    android:layout_alignParentTop="true"
    android:text="@string/ktv_xitongguanli"
    style="@style/BoldWhiteText24"
    android:gravity="center"
        />
<LinearLayout 
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:layout_marginTop="60dp"
    android:layout_marginBottom="55dp"
    android:layout_marginLeft="30dp"
    android:layout_marginRight="30dp"
    >
    <LinearLayout 
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
     android:layout_weight="1"
    >
    <RelativeLayout 
        android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_weight="1"
    android:id="@+id/sys_gongbo"
        >
        <ImageView 
            android:id="@+id/gongbo"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:background="@drawable/sys_gongbo"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            />
        <TextView 
            android:layout_below="@id/gongbo"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:text="@string/ktv_set_gongbo"
            android:gravity="center"
            style="@style/NormalWhiteText16"
            />
        </RelativeLayout>
         <RelativeLayout 
        android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_weight="1"
     android:id="@+id/sys_u_jiage"
        >
        <ImageView 
            android:background="@drawable/sys_u_pan"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
             android:layout_width="75dp"
            android:layout_height="75dp"
            android:id="@+id/ujiage"
            />
        <TextView 
            android:text="@string/ktv_set_upanjiage"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
             android:gravity="center"
            style="@style/NormalWhiteText16"
            android:layout_below="@id/ujiage"
            />
        
    </RelativeLayout>
     <RelativeLayout 
        android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_weight="1"
     android:id="@+id/sys_ge_huanyuan"
        >
        <ImageView 
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
              android:layout_width="75dp"
            android:layout_height="75dp"
            android:id="@+id/huanyuan"
            android:background="@drawable/sys_gedan"
            />
        <TextView 
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
             android:gravity="center"
            style="@style/NormalWhiteText16"
            android:layout_below="@id/huanyuan"
            android:text="@string/ktv_set_gedanhuanyuan"
            />
        
    </RelativeLayout>
    
      <RelativeLayout 
        android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_weight="1"
     android:id="@+id/sys_ge_shanchu"
        >
        <ImageView 
             android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
             android:layout_width="75dp"
            android:layout_height="75dp"
            android:id="@+id/shanchu"
            android:background="@drawable/sys_delete"
            />
        <TextView 
             android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
             android:gravity="center"
            style="@style/NormalWhiteText16"
            android:layout_below="@id/shanchu"
            android:text="@string/ktv_set_gequshanchu"
            />
        
    </RelativeLayout>
    
      
       <RelativeLayout 
        android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_weight="1"
     android:id="@+id/sys_ge_xiugai"
        >
        <ImageView 
              android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
              android:layout_width="75dp"
            android:layout_height="75dp"
            android:id="@+id/xiugai"
            android:background="@drawable/sys_song_mod"
            />
        <TextView 
             android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
             android:gravity="center"
            style="@style/NormalWhiteText16"
            android:layout_below="@id/xiugai"
            android:text="@string/ktv_set_gequxiugai"
            />
        
    </RelativeLayout>
    
</LinearLayout>
    
      <LinearLayout 
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
     android:layout_weight="1"
    >
    <RelativeLayout 
        android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_weight="1"
    android:id="@+id/sys_shuchu"
        >
        <ImageView 
             android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
               android:layout_width="75dp"
            android:layout_height="75dp"
            android:id="@+id/shuchu"
            android:background="@drawable/sys_shuchu"
            />
        <TextView 
             android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
             android:gravity="center"
            style="@style/NormalWhiteText16"
            android:layout_below="@id/shuchu"
            android:text="@string/ktv_set_shuchushezhi"
            />
        </RelativeLayout>
         <RelativeLayout 
        android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_weight="1"
     android:id="@+id/sys_zimu"
        >
        <ImageView 
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
              android:layout_width="75dp"
            android:layout_height="75dp"
            android:id="@+id/zimu"
            android:background="@drawable/sys_zimu"
            />
        <TextView 
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
             android:gravity="center"
            style="@style/NormalWhiteText16"
            android:layout_below="@id/zimu"
            android:text="@string/ktv_set_zimushezhi"
            />
        
    </RelativeLayout>
     <RelativeLayout 
        android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_weight="1"
     android:id="@+id/sys_shengji"
        >
        <ImageView 
             android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
              android:layout_width="75dp"
            android:layout_height="75dp"
            android:id="@+id/shengji"
