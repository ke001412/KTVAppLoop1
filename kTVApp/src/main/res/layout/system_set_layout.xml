            android:background="@drawable/sys_wangluo"
            />
        <TextView 
             android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
             android:gravity="center"
            style="@style/NormalWhiteText16"
            android:layout_below="@id/wangluo"
            android:text="@string/ktv_set_wangluoshezhi"
            />
        
    </RelativeLayout>
    
</LinearLayout>
</LinearLayout>

<com.sz.ktv.view.widget.KtvBottomPageWidget
    android:layout_width="match_parent"
    	android:layout_height="45dp"
    	android:id="@+id/sys_back"
    	android:layout_alignParentBottom="true"
    	android:layout_marginBottom="10dp"
    />

</RelativeLayout>